<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" id="WebApp_ID" version="2.5">
  <display-name>SeventhServletConfigObject</display-name>
  <servlet>
    <description></description>
    <display-name>ServletConfigDemo</display-name>
    <servlet-name>ServletConfigDemo</servlet-name>
    <servlet-class>in.ineuron.controller.ServletConfigDemo</servlet-class>
    <init-param>
      <param-name>jdbcUrl</param-name>
      <param-value>jdbc:mysql://localhost:3306/newone</param-value>
    </init-param>
    <init-param>
      <param-name>User</param-name>
      <param-value>root</param-value>
    </init-param>
    <init-param>
      <param-name>password</param-name>
      <param-value>root</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletConfigDemo</servlet-name>
    <url-pattern>/demo</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ServletConfigTest</display-name>
    <servlet-name>ServletConfigTest</servlet-name>
    <servlet-class>in.ineuron.controller.ServletConfigTest</servlet-class>
    <init-param>
    	<param-name>Company</param-name>
    	<param-value>iNeuron</param-value>
    </init-param>
    <init-param>
    	<param-name>Course</param-name>
    	<param-value>Enterprise Java</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>ServletConfigTest</servlet-name>
    <url-pattern>/test</url-pattern>
  </servlet-mapping>
</web-app>