<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<!-- Mapping using xml like @Entity,@Id, @Column -->
<hibernate-mapping>
  <class name="in.ineuron.Model.Employee" table="employee">
    <id name="empId" type="java.lang.Integer" column="eid">
   	 <generator class="increment"></generator></id>
    <property name="empName" type="java.lang.String" length="20" column="ename"> </property>
    <property name="empSalary" type="java.lang.Integer" column="esalary"></property>
  </class>
</hibernate-mapping>